{
  "homepage": ".",
  "name": "optc-box-manager",
  "version": "2.8.0",
  "private": true,
  "scripts": {
    "extract-optcdb": "node ./src/scripts/index.js",
    "extract-image": "node ./src/scripts/cropImage.mjs",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "prebuild": "npm run extract-optcdb && npm run extract-image",
    "postbuild": "copyfiles -f \"public/opencv-*.js\" build/static/js",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@styled-system/css": "^5.1.5",
    "@styled-system/should-forward-prop": "^5.1.5",
    "@styled-system/theme-get": "^5.1.2",
    "diff": "^5.1.0",
    "rc-slider": "^10.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^8.0.7",
    "react-scripts": "^5.0.1",
    "rehype-raw": "^6.1.1",
    "styled-system": "^5.1.5",
    "typescript": "^4.9.5",
    "uuid": "^9.0.0",
    "web-vitals": "^3.3.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/diff": "^5.0.3",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.3",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "@types/styled-system": "^5.1.16",
    "@types/styled-system__css": "^5.0.17",
    "@types/styled-system__should-forward-prop": "^5.1.2",
    "@types/styled-system__theme-get": "^5.0.2",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "ajv": "^8.12.0",
    "copyfiles": "^2.4.1",
    "eslint-config-react-app": "^7.0.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-n": "^16.0.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "jsonc-parser": "^3.2.0",
    "react-test-renderer": "^18.2.0",
    "sharp": "^0.32.1",
    "source-map-explorer": "^2.5.3"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "not IE 11"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "testMatch": [
      "**/(components|hooks|pages|services)/**/__tests__/**/*.[jt]s?(x)",
      "**/(components|hooks|pages|services)/**/?(*.)+(spec|test).[jt]s?(x)"
    ]
  }
}
