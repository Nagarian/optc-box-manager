{
  "homepage": ".",
  "name": "optc-box-manager",
  "version": "3.1.0",
  "type": "module",
  "private": true,
  "scripts": {
    "extract-optcdb": "node ./src/scripts/index.js",
    "extract-image": "node ./src/scripts/cropImage.js",
    "analyze": "source-map-explorer 'dist/assets/*.js' --no-border-checks",
    "prebuild": "npm run extract-optcdb && npm run extract-image",
    "ts-check": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "format": "prettier . --write",
    "start": "vite",
    "build": "tsc && vite build",
    "test": "vitest",
    "serve": "vite preview"
  },
  "dependencies": {
    "@emotion/react": "^11.13.0",
    "@emotion/styled": "^11.11.5",
    "@styled-system/css": "^5.1.5",
    "@styled-system/should-forward-prop": "^5.1.5",
    "diff": "^5.2.0",
    "rc-slider": "^10.6.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-markdown": "^9.0.1",
    "rehype-raw": "^7.0.0",
    "styled-system": "^5.1.5",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.2.0",
    "@techstark/opencv-js": "^4.9.0-release.3",
    "@testing-library/jest-dom": "^6.4.5",
    "@testing-library/react": "^15.0.7",
    "@types/diff": "^5.2.1",
    "@types/node": "^20.12.12",
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.3.0",
    "@types/styled-system": "^5.1.22",
    "@types/styled-system__css": "^5.0.21",
    "@types/styled-system__should-forward-prop": "^5.1.4",
    "@types/styled-system__theme-get": "^5.0.4",
    "@types/uuid": "^9.0.8",
    "@vitejs/plugin-react": "^4.2.1",
    "ajv": "^8.13.0",
    "eslint": "^8.57.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-simple-import-sort": "^12.1.0",
    "eslint-plugin-testing-library": "^6.2.2",
    "jsdom": "^24.0.0",
    "jsonc-parser": "^3.2.1",
    "prettier": "^3.2.5",
    "react-test-renderer": "^18.3.1",
    "sharp": "^0.33.4",
    "source-map-explorer": "^2.5.3",
    "ts-json-schema-generator": "^2.1.1",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.9.0",
    "vite": "^5.2.11",
    "vite-plugin-pwa": "^0.20.0",
    "vite-plugin-svgr": "^4.2.0",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^1.6.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "not IE 11"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=20"
  }
}
