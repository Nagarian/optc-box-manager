{
  "homepage": ".",
  "name": "optc-box-manager",
  "version": "3.1.0",
  "type": "module",
  "private": true,
  "scripts": {
    "extract-optcdb": "node ./src/scripts/extract-optcdb.js",
    "extract-image": "tsx ./src/scripts/extract-image.ts",
    "extract-shipdb": "tsx ./src/scripts/extract-shipdb.ts",
    "analyze": "source-map-explorer 'dist/assets/*.js' --no-border-checks",
    "prebuild": "npm run extract-optcdb && npm run extract-shipdb && npm run extract-image",
    "ts-check": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "format": "prettier . --write",
    "start": "vite",
    "build": "tsc && vite build",
    "test": "vitest",
    "serve": "vite preview"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@styled-system/css": "^5.1.5",
    "@styled-system/should-forward-prop": "^5.1.5",
    "diff": "^8.0.2",
    "rc-slider": "^11.1.8",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-markdown": "^10.1.0",
    "remark-gfm": "^4.0.1",
    "styled-system": "^5.1.5",
    "uuid": "^12.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@techstark/opencv-js": "^4.11.0-release.1",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^22.17.0",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@types/styled-system": "^5.1.23",
    "@types/styled-system__css": "^5.0.21",
    "@types/styled-system__should-forward-prop": "^5.1.4",
    "@types/styled-system__theme-get": "^5.0.4",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-react": "^4.7.0",
    "ajv": "^8.17.1",
    "eslint": "^9.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-testing-library": "^7.6.3",
    "jsdom": "^26.1.0",
    "jsonc-parser": "^3.3.1",
    "prettier": "^3.6.2",
    "react-test-renderer": "^19.1.1",
    "sharp": "^0.34.3",
    "source-map-explorer": "^2.5.3",
    "ts-json-schema-generator": "^2.4.0",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.38.0",
    "vite": "^7.0.6",
    "vite-plugin-pwa": "^1.0.2",
    "vite-plugin-svgr": "^4.3.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "not IE 11"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=22"
  }
}
