  "type": "module",
  "private": true,
  "scripts": {
    "extract-optcdb": "node ./src/scripts/index.js",
    "extract-image": "node ./src/scripts/cropImage.js",
    "analyze": "source-map-explorer 'dist/assets/*.js' --no-border-checks",
    "prebuild": "npm run extract-optcdb && npm run extract-image",
